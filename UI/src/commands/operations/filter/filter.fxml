<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" minHeight="-Infinity" minWidth="-Infinity" vgap="10.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="commands.operations.filter.FilterController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label prefHeight="18.0" prefWidth="133.0" text="Select Table Boundaries:" />
      <Label text="Column:" GridPane.rowIndex="1" />
      <ComboBox fx:id="comboBoxColumn1" onAction="#columnAction" prefWidth="150.0" promptText="Select Column" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button alignment="CENTER" mnemonicParsing="false" text="+" GridPane.columnIndex="4" GridPane.rowIndex="1" />
      <Label text="Values:" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <TextField fx:id="textFieldValues" prefHeight="26.0" prefWidth="200.0" promptText="Example: Red, Green, Blue" GridPane.columnIndex="3" GridPane.rowIndex="1" />
      <Button fx:id="buttonSubmit" defaultButton="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#submitAction" text="Submit" GridPane.columnIndex="5" GridPane.rowIndex="2" />
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1">
         <children>
            <TextField fx:id="textFieldFrom" alignment="CENTER" prefHeight="26.0" prefWidth="53.0" promptText="Exp: A1">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="textFieldSeparator" alignment="CENTER" editable="false" prefHeight="26.0" prefWidth="24.0" text="..">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="textFieldTo" alignment="CENTER" prefHeight="26.0" prefWidth="53.0" promptText="Exp: D4">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
