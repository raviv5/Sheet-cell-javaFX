<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<ScrollPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ranges.RangesController">
   <content>
      <AnchorPane>
         <children>
            <GridPane alignment="CENTER" hgap="5.0" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="200.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
              </rowConstraints>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <children>
                  <Button fx:id="buttonSetColumnsSize" mnemonicParsing="false" onAction="#setColumnsSizeAction" prefHeight="67.0" prefWidth="214.0" text="Add Range" GridPane.rowIndex="1" />
                  <Button fx:id="bottonSetColumnAlignment" mnemonicParsing="false" onAction="#setColumnAlignmentAction" prefHeight="67.0" prefWidth="214.0" text="Delete Range" GridPane.rowIndex="2" />
                  <Button fx:id="buttonSetCellDesign" mnemonicParsing="false" onAction="#setCellDesignAction" prefHeight="67.0" prefWidth="214.0" text="Show Range" GridPane.rowIndex="3" />
                  <Label alignment="CENTER" prefHeight="18.0" prefWidth="238.0" text="Range Operations" wrapText="true" />
                  <TextArea editable="false" prefHeight="200.0" prefWidth="200.0" promptText="Add Range..." wrapText="true" GridPane.rowIndex="4" />
               </children>
            </GridPane>
         </children>
      </AnchorPane>
   </content>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</ScrollPane>
